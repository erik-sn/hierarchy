  "jest": {
    "verbose": false,
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/actions/**/*.{js,jsx}",
      "src/components/**/*.{js,jsx}",
      "src/reducers/**/*.{js,jsx}",
      "src/utils/**/*.{js,jsx}"
